{
  "name": "paper-elements-web-types",
  "version": "0.1.1",
  "license": "BSD-3-Clause",
  "description": "Web-types for paper-elements",
  "keywords": [
    "web-types",
    "paper-elements",
    "completion",
    "autocompletion"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpradelle/paper-elements-web-types.git"
  },
  "scripts": {
    "web-types-gen": "wca-webtypes node_modules/@polymer/app-* node_modules/@polymer/gold-* node_modules/@polymer/iron-* node_modules/@polymer/marked-* node_modules/@polymer/neon-* node_modules/@polymer/paper-* node_modules/@polymer/prism-* --path-as-absolute --cwd node_modules --discover-node-modules --format webtypes --out-file ./src/gen/paper-elements-template-gen-web-types.json --exclude-classes='LegacyElementMixin' --webtypes-config='{\"name\": \"paper-elements-web-types\", \"version\": \"'$npm_package_version'\", \"default-icon\": \"icon/polymer.png\", \"description-markup\": \"markdown\", \"framework\": \"-\"}'",
    "build": "npm run build:polymer; npm run build:lit",
    "build:polymer": "export FW_NAME=polymer; export FRAMEWORK=@polymer/polymer; npm run build:generic",
    "build:lit": "export FW_NAME=lit; export FRAMEWORK=lit; npm run build:generic",
    "build:generic": "npm run build:copy; npm run build:update",
    "build:copy": "cp src/gen/paper-elements-template-gen-web-types.json paper-elements-$FW_NAME-gen-web-types.json; cp src/paper-elements-template-extra-web-types.json paper-elements-$FW_NAME-extra-web-types.json",
    "build:update": "replace-json-property ./paper-elements-$FW_NAME-gen-web-types.json framework \"$FRAMEWORK\" -s 4; replace-json-property ./paper-elements-$FW_NAME-extra-web-types.json framework \"$FRAMEWORK\" -s 2",
    "bump": "npm run bump:polymer; npm run bump:lit",
    "bump:polymer": "export FW_NAME=polymer; npm run bump:generic",
    "bump:lit": "export FW_NAME=lit; npm run bump:generic",
    "bump:generic": "replace-json-property ./paper-elements-$FW_NAME-gen-web-types.json version $npm_package_version -s 4; replace-json-property ./paper-elements-$FW_NAME-extra-web-types.json version $npm_package_version -s 2",
    "version": "npm run build && npm run bump && git add *-web-types.json"
  },
  "devDependencies": {
    "@polymer/app-layout": "^3.1.0",
    "@polymer/app-route": "^3.0.2",
    "@polymer/app-storage": "^3.0.3",
    "@polymer/gold-cc-cvc-input": "^3.0.2",
    "@polymer/gold-cc-expiration-input": "^3.0.1",
    "@polymer/gold-cc-input": "^3.0.1",
    "@polymer/gold-email-input": "^0.0.3",
    "@polymer/gold-phone-input": "^3.0.1",
    "@polymer/gold-zip-input": "^3.0.1",
    "@polymer/iron-a11y-announcer": "^3.2.0",
    "@polymer/iron-a11y-keys": "^3.0.1",
    "@polymer/iron-a11y-keys-behavior": "^3.0.1",
    "@polymer/iron-ajax": "^3.0.1",
    "@polymer/iron-autogrow-textarea": "^3.0.3",
    "@polymer/iron-behaviors": "^3.0.1",
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@polymer/iron-doc-viewer": "^4.0.1",
    "@polymer/iron-dropdown": "^3.0.1",
    "@polymer/iron-fit-behavior": "^3.1.0",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-iconset": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-image": "^3.0.2",
    "@polymer/iron-jsonp-library": "^3.0.1",
    "@polymer/iron-list": "^3.1.0",
    "@polymer/iron-location": "^3.0.2",
    "@polymer/iron-media-query": "^3.0.1",
    "@polymer/iron-overlay-behavior": "^3.0.3",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-swipeable-container": "^3.0.1",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@polymer/paper-badge": "^3.1.0",
    "@polymer/paper-behaviors": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-dialog-behavior": "^3.0.1",
    "@polymer/paper-dialog-scrollable": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.2.0",
    "@polymer/paper-fab": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.2.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.1.0",
    "@polymer/paper-progress": "^3.0.1",
    "@polymer/paper-radio-button": "^3.0.1",
    "@polymer/paper-radio-group": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.2",
    "@polymer/paper-slider": "^3.0.1",
    "@polymer/paper-spinner": "^3.0.2",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-swatch-picker": "^3.0.1",
    "@polymer/paper-tabs": "^3.1.0",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-toolbar": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "replace-json-property": "^1.8.0",
    "web-component-analyzer-webtypes": "^2.0.4"
  },
  "web-types": [
    "./paper-elements-lit-gen-web-types.json",
    "./paper-elements-lit-extra-web-types.json",
    "./paper-elements-polymer-gen-web-types.json",
    "./paper-elements-polymer-extra-web-types.json"
  ],
  "files": [
    "*.json",
    "README.md",
    "yarn.lock",
    "icon/**"
  ]
}
